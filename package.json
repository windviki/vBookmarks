{
  "name": "vbookmarks",
  "version": "2.0.0",
  "description": "Modern Chrome extension for enhanced bookmark management",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "package": "node scripts/package.js",
    "package:dev": "node scripts/package.js dev",
    "translate": "node scripts/translate.js",
    "translate:check": "node scripts/translate.js check",
    "translate:missing": "node scripts/translate.js missing",
    "analyze": "node scripts/analyze.js",
    "clean": "node scripts/clean.js",
    "type-check": "tsc --noEmit",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "chrome-extension",
    "bookmarks",
    "productivity",
    "modern",
    "typescript"
  ],
  "author": "vBookmarks Team",
  "license": "MIT",
  "dependencies": {
    "archiver": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "esbuild": "^0.20.2",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/vbookmarks.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/vbookmarks/issues"
  },
  "homepage": "https://github.com/your-username/vbookmarks#readme"
}